---
// Imports
import { getCollection } from "astro:content";
import { slugify } from "../../scripts/helpers/slugify";
import Layout from '../../layouts/Layout.astro';
import PageService from "../../components/PageService.astro";

// Static Data
const metaTitle = "Graphic Design";
const metaDescription = "We deliver eye-catching branding materials that are tailored to meet your business objectives.";

// Dynamic Data & Logic
const items = (await getCollection(slugify(metaTitle)));
---

<Layout {metaTitle} {metaDescription} variant="call-to-action">
	<PageService {metaTitle} {metaDescription} {items} />
</Layout>
